; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "AnimatedSprite",
"class": "AnimatedSpriteClip",
"language": "GDScript",
"path": "res://core/class/AnimatedSpriteClip.gd"
}, {
"base": "AnimationPlayer",
"class": "AnimationPlayerRenderer",
"language": "GDScript",
"path": "res://core/class/AnimationPlayerRenderer.gd"
} ]
_global_script_class_icons={
"AnimatedSpriteClip": "",
"AnimationPlayerRenderer": ""
}

[application]

config/name="VideoRender2D"
config/icon="res://core/icon.png"

[autoload]

VideoRenderServer="*res://core/singleton/VideoRenderServer.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[rendering]

environment/default_clear_color=Color( 1, 1, 1, 1 )
environment/default_environment="res://core/default_env.tres"
